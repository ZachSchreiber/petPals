<h2>PetPals News</h2>
{{#each reverse as |news|}}
{{#if news.title}}
<div id="blog-container">
  <div id="blog-title">
    <ul>
      <li>
        <p>{{news.date}}</p>
      </li>
      <li>
      <i class="fa fa-paw paw-link" aria-hidden="true"></i><a id="news-link" href="{{news.link}}">{{news.title}}</a>
      </li>
    </ul>
  </div>
  <div id="blog-body">
    <p>{{news.summary}}</p>
  </div>
</div>
{{/if}}
{{/each}}
