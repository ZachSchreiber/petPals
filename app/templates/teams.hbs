
<div class="content-full">
<h2>Teams</h2>
<div id="teams-main">
  {{#each reverse as |teams|}}
  {{#if teams.name}}
    <div id="teams-container">
      <div class="team-img-cont">
      <div class="team-img">
            <img class="teams-Img" src="{{teams.image}}" />
        </div>
        <div id="teams-name">
            <h3 id="name">{{teams.name}}</h3>
        </div>
      </div>
      <div id="teams-bio">
        <p id='bio'>{{teams.bio}}</p>
      </div>
    </div>
    {{/if}}
  {{/each}}
</div>
</div>

{{outlet}}
